{
  "rules": {
    "flamelink": {
      ".read": "true",
      ".write": "auth != null",
      "environments": { "production": {"content":{"courses":{"en-US": {
        ".indexOn": "slug"
      }}}}}
    },
    "accounts": {
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid"
      },
      ".indexOn": "email"
    },
    "inquiries": {
      "$uid": {
        ".write": "true"
      }
    }
  }
}
