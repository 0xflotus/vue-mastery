{
  "rules": {
    "flamelink": {
      ".read": "auth != null",
      ".write": "auth != null"
    },
    "accounts": {
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid"
      }
    }
  }
}
 
